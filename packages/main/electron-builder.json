{
    "productName": "ArDrive",
    "appId": "com.ardrive.ArDrive",
    "directories": {
        "buildResources": "assets"
    },
    "files": [
        {
            "from": "../renderer/build/",
            "to": "./renderer/",
            "filter": [
                "**/*"
            ]
        },
        {
            "from": "./build/",
            "to": "./",
            "filter": [
                "**/*"
            ]
        },
        "package.json"
    ],
    "dmg": {
        "contents": [
            {
                "x": 130,
                "y": 220
            },
            {
                "x": 410,
                "y": 220,
                "type": "link",
                "path": "/Applications"
            }
        ]
    },
    "win": {
        "target": [
            "nsis",
            "msi"
        ]
    },
    "linux": {
        "target": [
            "deb",
            "rpm",
            "AppImage"
        ],
        "category": "Development"
    }
}